    Top = 372
  end
  object qr_prt_product: TADOQuery
    Connection = DM.ADOConnection1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'select a.all_products_code,a.product_model_code,a.shipping_date,' +
        'a.manufacture_date,a.warrentycard_code'
      
        ',y.product_chinese_name,y.product_english_name,x.product_gross,x' +
        '.formal_size'
      ',t.part_model_code,t.serial_no from all_products a'
      
        'left outer join product_model l on a.product_model_code= l.produ' +
        'ct_model_code'
      
        'left outer join product_box x on l.product_category_code=x.produ' +
        'ct_category_code'
      
        'left outer join all_products_mainpart t on t.all_products_code=a' +
        '.all_products_code'
      
        'left outer join part_model e on e.part_model_code=t.part_model_c' +
        'ode'
      
        'left outer join product_category y on y.product_category_code=l.' +
        'product_category_code'
      
        'where CONVERT(CHAR,a.manufacture_date,110)=CONVERT(CHAR,getdate(' +
        '),110)')
    Left = 408
    Top = 372
    object qr_prt_productall_products_code: TStringField
      FieldName = 'all_products_code'
    end
    object qr_prt_productproduct_model_code: TStringField
      FieldName = 'product_model_code'
    end
    object qr_prt_productshipping_date: TDateTimeField
      FieldName = 'shipping_date'
    end
    object qr_prt_productmanufacture_date: TDateTimeField
      FieldName = 'manufacture_date'
    end
    object qr_prt_productwarrentycard_code: TStringField
      FieldName = 'warrentycard_code'
    end
    object qr_prt_productproduct_chinese_name: TStringField
      FieldName = 'product_chinese_name'
      Size = 50
    end
    object qr_prt_productproduct_english_name: TStringField
      FieldName = 'product_english_name'
      Size = 50
    end
    object qr_prt_productproduct_gross: TStringField
      FieldName = 'product_gross'
      Size = 50
    end
    object qr_prt_productformal_size: TStringField
      FieldName = 'formal_size'
      Size = 50
    end
    object qr_prt_productpart_model_code: TStringField
      FieldName = 'part_model_code'
    end
    object qr_prt_productserial_no: TStringField
      FieldName = 'serial_no'
      Size = 50
    end
