  object qr_order: TADOQuery
    Connection = DM.ADOConnection1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'select a.order_code,c.cust_name from all_products a'
      ' left outer join sales_order s on a.order_code= s.order_code'
      ' left outer join cust c on s.cust_code= c.cust_code'
      
        ' where CONVERT(CHAR,a.manufacture_date,110)=CONVERT(CHAR,getdate' +
        '(),110)')
    Left = 344
    Top = 372
    object qr_orderorder_code: TStringField
      FieldName = 'order_code'
    end
    object qr_ordercust_name: TStringField
      FieldName = 'cust_name'
      Size = 100
    end
  end
  object ds_order: TDataSource
    DataSet = qr_order
    Left = 376
    Top = 372
  end
